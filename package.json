{
  "name": "passing-notes-ui",
  "version": "0.0.0",
  "description": "A middleware for delivering code to the browser during development",
  "keywords": [
    "bundler",
    "compile",
    "esbuild"
  ],
  "homepage": "https://github.com/vinsonchuong/passing-notes-ui",
  "bugs": "https://github.com/vinsonchuong/passing-notes-ui/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/passing-notes-ui",
  "main": "index.js",
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "type": "module",
  "dependencies": {
    "@babel/code-frame": "^7.14.5",
    "cjs-module-lexer": "^1.2.1",
    "enhanced-resolve": "^5.8.2",
    "esbuild": "^0.12.15",
    "lodash": "^4.17.21",
    "passing-notes-static": "^1.2.0"
  },
  "peerDependencies": {
    "passing-notes": "^6.2.0"
  },
  "devDependencies": {
    "@material-ui/core": "^4.12.1",
    "ava": "^3.15.0",
    "ava-patterns": "^2.4.1",
    "htm": "^3.1.0",
    "passing-notes": "^6.5.0",
    "puppet-strings": "^4.1.0",
    "puppet-strings-chrome": "^2.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-release": "^17.4.4",
    "xo": "^0.41.0"
  },
  "ava": {
    "verbose": true,
    "timeout": "60s"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "rules": {
      "no-await-in-loop": "off",
      "import/no-anonymous-default-export": "off",
      "unicorn/prevent-abbreviations": "off"
    },
    "globals": [
      "window",
      "document"
    ]
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
