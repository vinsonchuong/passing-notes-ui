{
  "name": "passing-notes-ui",
  "version": "0.0.0",
  "description": "A middleware for delivering code to the browser during development",
  "keywords": [
    "bundler",
    "compile",
    "esbuild"
  ],
  "homepage": "https://github.com/vinsonchuong/passing-notes-ui",
  "bugs": "https://github.com/vinsonchuong/passing-notes-ui/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/passing-notes-ui",
  "main": "index.js",
  "bin": {
    "serve-ui": "cli/serve-ui.js"
  },
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "type": "module",
  "dependencies": {
    "@babel/code-frame": "^7.18.6",
    "cjs-module-lexer": "^1.2.2",
    "enhanced-resolve": "^5.12.0",
    "es-module-lexer": "^1.2.0",
    "esbuild": "^0.17.10",
    "lodash": "^4.17.21",
    "passing-notes": "^6.5.7",
    "passing-notes-static": "^1.3.0",
    "strip-indent": "^4.0.0"
  },
  "devDependencies": {
    "@material-ui/core": "^4.12.4",
    "ava": "^4.3.3",
    "ava-patterns": "^3.2.0",
    "goober": "^2.1.12",
    "htm": "^3.1.1",
    "p-event": "^5.0.1",
    "pify": "^6.1.0",
    "puppet-strings": "^4.1.3",
    "puppet-strings-chrome": "^2.0.3",
    "quick-install": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^20.1.0",
    "xo": "^0.53.1"
  },
  "ava": {
    "verbose": true,
    "timeout": "60s"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "rules": {
      "no-await-in-loop": "off",
      "import/no-anonymous-default-export": "off",
      "unicorn/prevent-abbreviations": "off"
    },
    "globals": [
      "window",
      "document"
    ]
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "packageManager": "yarn@3.2.1"
}
