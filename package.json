{
  "name": "passing-notes-ui",
  "version": "0.0.0",
  "description": "A middleware for delivering code to the browser during development",
  "keywords": [
    "bundler",
    "compile",
    "esbuild"
  ],
  "homepage": "https://github.com/vinsonchuong/passing-notes-ui",
  "bugs": "https://github.com/vinsonchuong/passing-notes-ui/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/passing-notes-ui",
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "type": "module",
  "dependencies": {
    "@babel/code-frame": "^7.10.4",
    "cjs-module-lexer": "^1.0.0",
    "enhanced-resolve": "^5.4.0",
    "esbuild": "^0.8.21",
    "lodash": "^4.17.20",
    "passing-notes-static": "^1.2.0"
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.2",
    "ava": "^3.14.0",
    "ava-patterns": "^2.4.1",
    "htm": "^3.0.4",
    "passing-notes": "^6.3.0",
    "puppet-strings": "^4.0.1",
    "puppet-strings-chrome": "^2.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "semantic-release": "^17.3.0",
    "xo": "^0.36.1"
  },
  "ava": {
    "verbose": true
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "rules": {
      "no-await-in-loop": "off",
      "import/extensions": "off",
      "import/no-anonymous-default-export": "off",
      "import/no-useless-path-segments": "off",
      "unicorn/import-index": "off",
      "unicorn/prevent-abbreviations": "off"
    },
    "globals": [
      "window",
      "document"
    ]
  }
}
